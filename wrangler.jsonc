{
  "$schema": "./node_modules/wrangler/config-schema.json",
  "main": ".open-next/worker.js",

  // Worker name should match the Worker created by Cloudflare Builds.
  // Your CI expected: "hace"
  "name": "hace",

  // MUST NOT be in the future (Cloudflare will reject it).
  // Use today's date or earlier (UTC-safe).
  "compatibility_date": "2026-02-08",
  "compatibility_flags": ["nodejs_compat"],

  "assets": {
    "directory": ".open-next/assets",
    "binding": "ASSETS"
  }
}
